{
  "name": "html-to-api-proxy",
  "version": "1.0.0",
  "description": "Dynamic API proxy for HTML-based websites",
  "main": "server.js",
  "scripts": {
    "start": "pm2 start server.js --name html-to-api-proxy",
    "dev": "pm2 start server.js --name html-to-api-proxy --watch",
    "stop": "pm2 stop html-to-api-proxy",
    "restart": "pm2 restart html-to-api-proxy",
    "delete": "pm2 delete html-to-api-proxy",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "axios-retry": "^4.0.2",
    "form-data": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "tough-cookie": "^4.1.3",
    "axios-cookiejar-support": "^4.0.7",
    "memory-cache": "^0.2.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "pm2": "^5.3.0",
    "jest": "^29.7.0",
    "eslint": "^8.53.0"
  },
  "keywords": ["api", "proxy", "html", "scraper", "dynamic"],
  "author": "",
  "license": "MIT"
}
